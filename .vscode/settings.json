{
    // "dotnet.defaultSolution": "",
    "vsintellicode.java.completionsEnabled": false,
    "vsintellicode.modify.editor.suggestSelection": "enabled",
    "vsintellicode.python.completionsEnabled": false,
    "vsintellicode.sql.completionsEnabled": false,
    "vsintellicode.typescript.completionsEnabled": false,
    "intellicodeApiExamples.python.enabled": false,
    "intellicodeApiExamples.typescript.enabled": false,
    "typescript.tsserver.web.projectWideIntellisense.enabled": false,
    "typescript.tsserver.web.typeAcquisition.enabled": false,
    "codeium.enableInComments": false,
    "codeium.enableCodeLens": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.inlineSuggest.edits.experimental.enabled": true,
    "editor.suggestOnTriggerCharacters": true,
    "terminal.integrated.suggest.quickSuggestions": false,
    "codeium.enableConfig": {
        "*": true
    },
    "editor.suggest.showWords": false,
    "codeium.disableSupercomplete": true,
    "javascript.suggest.enabled": false,
    "typescript.suggest.enabled": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.suggest.selectionMode": "always",
    "editor.inlineSuggest.enabled": false,
    "editor.suggest.preview": false,
    "editor.tabCompletion": "on",
    "emmet.preferences": {},
    "debug.confirmOnExit": "never",
    "debug.showSubSessionsInToolBar": true,
    "debug.console.closeOnEnd": false,
    "debug.console.acceptSuggestionOnEnter": "off",
    "debug.terminal.clearBeforeReusing": true,
    "terminal.explorerKind": "integrated",
    "debug.console.fontSize": 14,
    "csharp.debug.console": "integratedTerminal",
    "dotnet.defaultSolution": "ConsoleApp1.sln",
    "intellicode-completions-cdk.runAlongsideCopilot": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.useInlineCompletions": true,
    "omnisharp.enableAsyncCompletion": false,
    "editor.defaultFormatter": "ms-dotnettools.csdevkit",
    "csharp.inlayHints.enableInlayHintsForTypes": false,
    "dotnet.inlayHints.enableInlayHintsForParameters": false,
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "editor.inlineSuggest.suppressSuggestions": false,
    "[snippets]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}